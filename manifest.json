{
	// required
	"manifest_version" : 2,
	"name": "Tracking",
	"version": "1.0",
	"description": "This tracks where you spend your time online",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},

	"permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}