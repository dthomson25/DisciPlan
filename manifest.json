{
  "manifest_version": 2,

  "name": "DisciPlan",
  "description": "This extension will track and display internet usage",
  "version": "1.0",

"background": {
    "scripts": ["background.js"],
    "persistent": false
},
"browser_action": {
    "default_icon": "icon.png"
},
  "permissions": [
   "tabs"
   ]
}
