{
  "manifest_version": 2,

  "name": "DisciPlan",
  "description": "This extension will track and display internet usage",
  "version": "1.0",

"background": {
    "scripts": ["redirecting.js","track_usage.js","background.js"],
    "persistent": false
},
"browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*", "tabs", "https://*/*"
  ]
}
